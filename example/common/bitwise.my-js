use "std"

echo(#ABCDEF - #12345)
echo(-8 << 2)
echo(-8 >> 2)
echo(-8 >>> 2)

echo()
for a = 0, a <= 1, a = a + 1 {
    for b = 0, b <= 1, b = b + 1 {
        echo(a, " | ", b, " ", a | b)
        echo(a, " & ", b, " ", a & b)
        echo(a, " ^ ", b, " ", a ^ b)
    }
    echo(" ~", a, " ", ~a)
    echo(" !", a, " ", !a)
}